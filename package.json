{
  "name": "app",
  "private": true,
  "engines": {
    "node": ">=22.0.0 <23",
    "yarn": ">= 1.22.19 <2"
  },
  "packageManager": "yarn@1.22.19",
  "dependencies": {
    "@activeadmin/activeadmin": "2.13.0",
    "@client-side-validations/client-side-validations": "^0.3.0",
    "@hotwired/turbo": "^7.3.0",
    "@rails/ujs": "^7.1.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.9",
    "alpine-magic-helpers": "^1.2.2",
    "alpine-turbo-drive-adapter": "1.1.0",
    "alpinejs": "2.8.2",
    "autoprefixer": "^10.4.14",
    "core-js": "3.20.1",
    "esbuild": "^0.25.4",
    "fitvids": "1.0.2",
    "imagesloaded": "4.1.4",
    "infinite-scroll": "4.0.0",
    "jquery": "^3.7.0",
    "jquery-ui": "^1.13.2",
    "jquery-ujs": "^1.2.3",
    "marked": "4.0.10",
    "masonry-layout": "4.2.2",
    "photoswipe": "4.1.3",
    "postcss": "^8.4.31",
    "postcss-cli": "^11.0.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^15.1.0",
    "postcss-preset-env": "^9.0.0",
    "regenerator-runtime": "^0.14.1",
    "sass": "^1.63.6",
    "tailwindcss": "^3.3.3"
  },
  "scripts": {
    "build": "yarn build:js && yarn build:css",
    "build:js": "esbuild app/javascript/application.js app/javascript/blog.js app/javascript/photography.js app/javascript/active_admin.js --bundle --sourcemap --outdir=app/assets/builds --public-path=/assets",
    "build:js:watch": "yarn build:js --watch",
    "build:css": "postcss app/assets/stylesheets/application.css app/assets/stylesheets/contact-me.css --dir app/assets/builds && sass --load-path=node_modules app/assets/stylesheets/photography.sass:app/assets/builds/photography.css app/assets/stylesheets/active_admin.scss:app/assets/builds/active_admin.css app/assets/stylesheets/print.scss:app/assets/builds/print.css app/assets/stylesheets/pygment.sass:app/assets/builds/pygment.css",
    "build:css:watch": "postcss app/assets/stylesheets/application.css app/assets/stylesheets/contact-me.css --dir app/assets/builds --watch & sass --watch --load-path=node_modules app/assets/stylesheets/photography.sass:app/assets/builds/photography.css app/assets/stylesheets/active_admin.scss:app/assets/builds/active_admin.css app/assets/stylesheets/print.scss:app/assets/builds/print.css app/assets/stylesheets/pygment.sass:app/assets/builds/pygment.css"
  },
  "version": "0.1.0",
  "browserslist": [
    "defaults"
  ]
}
